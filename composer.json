{
  "name": "petervanderwal/adventofcode-cli",
  "type": "symfony-bundle",
  "license": "MIT",
  "description": "A Symfony bundle that helps to create puzzle implementations for Advent of Code",
  "require": {
    "php": ">=8.1",
    "nette/safe": "^0.9.0",
    "symfony/console": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-client": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/yaml": "^6.4"
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "PeterVanDerWal\\AdventOfCode\\Cli\\": "src/"
    }
  }
}
